{
  "name": "reactstartkit",
  "version": "1.0.0",
  "description": "React Start Kit",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node tools/preStart.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/setupServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"tests/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "security": "nsp check"
  },
  "author": "Peter Lee",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.3",
    "expect": "^22.4.3",
    "jquery": "^3.3.1",
    "jsdom": "^11.12.0",
    "lodash": "^4.17.10",
    "popper.js": "^1.14.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "colors": "^1.3.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^3.1.5",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "open": "0.0.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  }
}
